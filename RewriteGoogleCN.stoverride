name: RewriteGoogleCN
desc: Rewrite GoogleCN URLs to Google.
icon: https://raw.githubusercontent.com/RiverMud/Stash/master/icon/Google.PNG
category: Google

http:
  mitm:
    - g.cn
    - www.g.cn
    - google.cn
    - www.google.cn

  url-rewrite:
    - ^https?://(www\.)?g\.cn/?$ https://www.google.com 302
    - ^https?://(www\.)?google\.cn/?$ https://www.google.com 302
